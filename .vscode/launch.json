{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": "${command:pickArgs}"
        },
        {
            "name": "Deeppoly Example",
            "type": "debugpy",
            "request": "launch",
            "cwd": "${workspaceFolder}/verify/",
            "program": "exp.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "--net_fpath", "../nets/deeppoly.onnx",
                "--epsilon", "1",
                "--bp", "stm",
                "--opt", "mnlp"
            ]
        },
        {
            "name": "MNIST sigmoid 3x500 Example",
            "type": "debugpy",
            "request": "launch",
            "cwd": "${workspaceFolder}/verify/",
            "program": "exp.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "--net_fpath", "../nets/mnist_sigmoid_3_500.onnx",
                "--epsilon", "0.02",
                "--bp", "stm",
                "--opt", "mnlp"
            ]
        },
        {
            "name": "67 MNIST sigmoid 3x500 Example",
            "type": "debugpy",
            "request": "launch",
            "cwd": "${workspaceFolder}/verify/",
            "program": "exp.py",
            "console": "integratedTerminal",
            "justMyCode": false,
            "args": [
                "--net_fpath", "../nets/mnist_sigmoid_3_500.onnx",
                "--epsilon", "0.02",
                "--bp", "stm",
                "--opt", "mnlp",
                "--start", "67",
                "--num", "1"
            ]
        },
    ]
}

